export EDITOR="nvim"
alias v="nvim"
alias vim="nvim"
alias vv="vim ~/.config/nvim/init.vim"
alias vvp="vim ~/.config/nvim/plugins_config.vim"
alias vz="nvim ~/.zshrc"
alias vp="vim package.json"
alias vzh="vim ~/.zsh_history"
alias sz="source ~/.zshrc"
alias ns="npm show"
alias nrs="npm run start"
alias nrb="npm run build"
alias ni="npm install"
alias nis="npm install --save"
alias nisd="npm install --save-dev"
alias gdc="git diff --cached"
alias gdp="git diff package.json"
alias grm="git rebase main"
alias gdm="git diff main"
alias grc="git rebase --continue"
alias gra="git rebase --abort"
alias gcna="git commit --amend --no-verify"
alias ggpushn="ggpush --no-verify"
alias ggpushnf="ggpushn --force"
alias cdv="cd ~/.config/nvim"
alias nxgc="nx g @nx/angular:component --nameAndDirectoryFormat=as-provided"
alias rma="rm -rf .angular"
alias nxl="nx affected:lint --base=main"
alias nxt="nx affected:test --base=main"
alias config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'
